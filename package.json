{
  "name": "austs-two-click-node",
  "productName": "AustsTwoClickNode",
  "version": "0.1.0",
  "author": "Aust",
  "description": "An easy to use interface to run an Algorand or Voi consensus node.",
  "main": ".webpack/main",
  "scripts": {
    "create-icons": "electron-icon-builder --input=./assets/source.png --output=./assets",
    "lint": "eslint --ext .ts,.tsx .",
    "make": "electron-forge make",
    "make:linux": "electron-forge make --platform linux",
    "make:mac": "electron-forge make --platform darwin",
    "make:win": "electron-forge make --platform win32",
    "package": "electron-forge package",
    "package:linux": "electron-forge package --platform linux",
    "package:mac": "electron-forge package --platform darwin",
    "package:win": "electron-forge package --platform win32",
    "publish": "electron-forge publish",
    "publish:linux": "electron-forge publish --platform linux",
    "publish:mac": "electron-forge publish --platform darwin",
    "publish:win": "electron-forge publish --platform win32",
    "start": "electron-forge start --inspect-electron",
    "update-libs": "yarn upgrade algoseas-libs"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@electron-forge/plugin-webpack": "^6.2.1",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.0.0",
    "electron": "25.2.0",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "postcss": "^8.4.24",
    "postcss-font-magician": "^3.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@aust/react-flux": "^1.3.1",
    "algoseas-libs": "git+https://oauth2:5M-yAwqQJ_U-R-d7Pp9x@gitlab.probsttech.com/algoseas/libs.git",
    "electron-is-dev": "^2.0.0",
    "electron-persist-secure": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
